pipeline {
    agent any 
    stages {
        stage('Checkout') {
            steps {
                echo "Kode sudah diambil dari repositori Anda!"
            }
        }
        stage('Build & Test') {
            steps {
                // Di sini nanti Anda bisa menambahkan perintah build/test
                echo "Testing webhook triggers."
            }
        }
    }
}
